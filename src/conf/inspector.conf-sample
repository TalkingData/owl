controller_addr=127.0.0.1:10050

log_file=./logs/inspector.log
log_expire_days=3
log_level=4

max_packet_size=655350
max_task_buffer=655350
max_result_buffer=655350
worker_count=5

tsdb_addr=127.0.0.1:8080
tsdb_timeout=30

backend_tsdb=kairosdb

# 日志设置
# Emergency    -> 0
# Alert        -> 1
# Critical     -> 2
# Error        -> 3
# Warning      -> 4
# Notice       -> 5
# Info         -> 6
# Debug        -> 7