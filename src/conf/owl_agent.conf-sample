[main]
listen = 127.0.0.1:19575
exec_builtin_metrics_interval_secs = 60

[log]
level = debug
path = ../logs

[proxy]
address = 127.0.0.1:19577
call_proxy_timeout_secs = 10
download_plugin_timeout_secs = 90
list_plugins_interval_secs = 300
report_metric_interval_secs = 300
report_heartbeat_interval_secs = 60

[plugin]
plugin_dir = ../plugins
execute_untrusted_plugin = false